	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nyam.model.dao.FileDao">
	<insert id="insertFile" parameterType="FileDto" keyColumn="id" keyProperty="id" useGeneratedKeys="true">
		INSERT INTO image (
		    org_file
		) values (
		    #{orgFile}
		)
	</insert>
</mapper>